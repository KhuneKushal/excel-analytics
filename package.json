{
  "name": "excel-analytics",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node dist/excel-analytics/server/server.mjs",
    "build": "npx ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "vercel-build": "npm run build:ssr",
    "build:ssr": "ng build && ng run excel-analytics:server --configuration production",
    "serve:ssr": "node dist/excel-analytics/server/server.mjs",
    "dev:ssr": "ng run excel-analytics:serve-ssr",
    "prerender": "ng run excel-analytics:prerender"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "type": "module",
  "dependencies": {
    "@angular/animations": "^20.1.0",
    "@angular/cdk": "^20.1.5",
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/material": "^20.1.5",
    "@angular/platform-browser": "^20.1.0",
    "@angular/platform-server": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@angular/ssr": "^20.1.5",
    "@angular/cli": "^20.1.5",
    "@types/file-saver": "^2.0.7",
    "chart.js": "^4.5.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "expr-eval": "^2.0.2",
