{
  "version": 2,
  "buildCommand": "npm run build:ssr",
  "outputDirectory": "dist/excel-analytics",
  "builds": [
    {
      "src": "dist/excel-analytics/server/server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/excel-analytics/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/dist/excel-analytics/browser/assets/$1"
    },
    {
      "src": "/(.+\\.[a-zA-Z0-9]{2,}$)",
      "headers": { "cache-control": "public, max-age=31536000" },
      "dest": "/dist/excel-analytics/browser/$1"
    },
    {
      "src": "/_next/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/dist/excel-analytics/browser/_next/$1"
    },
    {
      "src": "/api/(.*)",
      "headers": { "cache-control": "no-cache, no-store, must-revalidate" },
      "dest": "/dist/excel-analytics/server/server.mjs"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/excel-analytics/server/server.mjs",
      "headers": {
        "x-frame-options": "DENY",
        "x-content-type-options": "nosniff",
        "x-xss-protection": "1; mode=block",
        "cache-control": "no-cache, no-store, must-revalidate"
      },
      "continue": true
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}