{
  "version": 2,
  "framework": "angular",
  "installCommand": "npm ci",
  "buildCommand": "npm run build:ssr",
  "outputDirectory": "dist/excel-analytics",
  "routes": [
    { 
      "src": "/assets/(.*)", 
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/browser/assets/$1" 
    },
    { 
      "handle": "filesystem"
    },
    {
      "src": ".*",
      "dest": "/server/main.server.mjs"
    }
  ]
}