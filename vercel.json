{
  "version": 2,
  "buildCommand": "npm run build:ssr",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "dist/excel-analytics/browser",
  "rewrites": [
    {
      "source": "/((?!api/.*).*)",
      "destination": "/api/server.js"
    }
  ],
  "functions": {
    "api/server.js": {
      "runtime": "nodejs22.x",
      "memory": 1024,
      "maxDuration": 10
    }
  }
}