{
  "version": 2,
  "buildCommand": "npm run build:ssr",
  "outputDirectory": "dist/excel-analytics",
  "builds": [
    {
      "src": "dist/excel-analytics/server/server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/excel-analytics/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/excel-analytics/browser/assets/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/_next/static/(.*)",
      "dest": "/dist/excel-analytics/browser/_next/static/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "dist/excel-analytics/server/server.mjs",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "src": "/(.*)",
      "dest": "dist/excel-analytics/server/server.mjs",
      "headers": {
        "x-frame-options": "DENY",
        "x-content-type-options": "nosniff",
        "x-xss-protection": "1; mode=block"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}