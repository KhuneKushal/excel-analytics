{
  "version": 2,
  "buildCommand": "npm run build:ssr",
  "outputDirectory": "dist/excel-analytics",
  "builds": [
    {
      "src": "dist/excel-analytics/server/server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/excel-analytics/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "^/assets/(.*)$",
      "dest": "/dist/excel-analytics/browser/assets/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "^/(.+\\.[a-zA-Z0-9]+)$",
      "dest": "/dist/excel-analytics/browser/$1",
      "headers": {
        "cache-control": "public, max-age=31536000"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/dist/excel-analytics/server/server.mjs",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/dist/excel-analytics/server/server.mjs"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}