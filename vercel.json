{
  "version": 2,
  "framework": "angular",
  "buildCommand": "npm run build:ssr",
  "outputDirectory": "dist/excel-analytics",
  "rewrites": [
    { "source": "/assets/(.*)", "destination": "/dist/excel-analytics/browser/assets/$1" },
    { "source": "/(.*)", "destination": "/dist/excel-analytics/server/server.mjs" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}